{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "morilog/jalali": "^2.2",
        "yangqi/htmldom": "dev-master",
        "intervention/image": "dev-master",
        "laravelcollective/html":"^5.4.0",
        "predis/predis": "^1.1",
        "yajra/laravel-datatables-oracle": "~7.0",
        "tymon/jwt-auth": "0.5.*",
        "barryvdh/laravel-ide-helper": "^2.3",
        "santigarcor/laratrust": "3.2.*",
        "torann/geoip": "^1.0",
        "monarobase/country-list": "^2.0",
        "geoip2/geoip2": "^2.5",
        "niklasravnsborg/laravel-pdf": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar":"2.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/HamahangHelper/API.php",
            "app/HamahangHelper/HomePage.php",
            "app/HamahangHelper/MenuTools.php",
            "app/HamahangHelper/Scheduler.php",
            "app/HamahangHelper/System.php",
            "app/HamahangHelper/TaskProjectProcess.php",
            "app/HamahangHelper/helpers.php",
            "app/HamahangHelper/bazaar_helpers.php",
            "app/HamahangHelper/FileManager_helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }

}
